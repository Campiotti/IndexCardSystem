<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Creator">
    <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Tab text="CardDecks">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <VBox id="creatorCDVBox" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <TextField id="creatorCDNameTxt" promptText="Name" />
                    <TextField id="creatorCDPassPTxt" promptText="Pass Percent" />
                    <TextField id="creatorCDCardsPRTxt" promptText="Cards Per Run" />
                    <Button id="creatorCDAddBtn" disable="true" mnemonicParsing="false" text="Add CardDeck" />
                </VBox>
                <TableView prefHeight="200.0" prefWidth="350.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn editable="false" prefWidth="75.0" text="Name" />
                        <TableColumn prefWidth="75.0" text="Questions" />
                        <TableColumn prefWidth="75.0" text="PassPercent" />
                        <TableColumn prefWidth="75.0" text="CPR" />
                    </columns>
                </TableView>
            </AnchorPane>
        </Tab>
        <Tab text="Q&amp;A">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </Tab>
        <Tab text="System">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Button layoutX="197.0" layoutY="280.0" mnemonicParsing="false" text="Return to Main Menu"
                        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                        AnchorPane.topAnchor="0.0" id="creatorSysBtn" onAction="#creatorSysBtnA">
                    <font>
                        <Font size="36.0"/>
                    </font>
                </Button>
            </AnchorPane>
        </Tab>
    </TabPane>
</AnchorPane>
